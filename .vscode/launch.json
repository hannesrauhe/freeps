  "version": "0.2.0",
  "configurations": [

    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "Launch freepsd",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": []
    },
    {
      "name": "denonoff",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "template", "-f", "denonoff"]
    },
    {
      "name": "denonon",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "template", "-f", "denonon"]
    },
    {
      "name": "fritz",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "fritz", "-f", "getdevicelistinfos"]
    },
    {
      "name": "fritz wakeup",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "fritz", "-f", "wakeup", "-a", "device=blub"]
    }
  ]
}