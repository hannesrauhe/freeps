  "version": "0.2.0",
  "configurations": [


    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "Launch freepsd",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": []
    },
    {
      "name": "wled",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "wled", "-f", "setPixel"]
    },
    {
      "name": "convert",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "template", "-f", "convert", "-a", "name=mqttaction"]
    },
    {
      "name": "fritztemplate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "template", "-f", "fritzgetdev"]
    },
    {
      "name": "wledoff",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "template", "-f", "wledoff"]
    },
    {
      "name": "fritz",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "fritz", "-f", "upnp", "-a", "serviceName=urn:dslforum-org:service:X_AVM-DE_Speedtest:1&actionName=GetInfo"]
    }
  ]
}