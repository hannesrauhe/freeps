  "version": "0.2.0",
  "configurations": [


    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "Launch freepsd",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": []
    },
    {
      "name": "wled SetPixel",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "wled", "-f", "sendCmd", "-a", "on=1"]
    },
    {
      "name": "weather",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "weather", "-f", "current"]
    },
    {
      "name": "fritz",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "fritz", "-f", "upnp", "-a", "serviceName=urn:dslforum-org:service:X_AVM-DE_Speedtest:1&actionName=GetInfo"]
    }
  ]
}