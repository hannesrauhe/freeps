  "version": "0.2.0",
  "configurations": [


    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "Launch freepsd",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-v"]
    },
    {
      "name": "wled DrawText",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "pixeldisplay", "-f", "DrawText", "-a", "Text=12:19&align=center", "-v"]
    },
    {
      "name": "wled SetPixel",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "pixeldisplay", "-f", "DrawPixel", "-a", "Color=%239929ff&ImageName=bla&X=12&Y=1", "-v"]
    },
    {
      "name": "store",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "store", "-f", "getNamespaces"]
    },
    {
      "name": "fritz",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/freepsd/freepsd.go",
      "args": ["-m", "fritz", "-f", "DiscoverHosts"]
    }
  ]
}